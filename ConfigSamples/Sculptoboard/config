beta_homing_retract_mm                       5                # "
gamma_homing_retract_mm                      5                # "

endstop_debounce_count                       100              # uncomment if you get noise on your endstops, default is 100

# Z-probe
#--------
#zprobe.enable                false               # set to true to enable a zprobe
zprobe.enable                true               # set to true to enable a zprobe
zprobe.probe_pin             3.26^           
zprobe.slow_feedrate         5                  # mm/sec probe feed rate
zprobe.debounce_count       100                # set if noisy
zprobe.fast_feedrate         100                # move feedrate
zprobe.probe_height          9                  # how much above bed to start probe NB only needed for G32 on delta

# associated with zprobe the leveling strategy to use
leveling-strategy.average-leveling.enable         true        # a leveling strategy that probes three points to define a plane and keeps the Z parallel to that plane
leveling-strategy.average-leveling.probe_points   4
leveling-strategy.average-leveling.point0         0.0,0.0   # the first probe point (x,y) optional may be defined with M557
leveling-strategy.average-leveling.point1         90.0,0.0   # the first probe point (x,y) optional may be defined with M557
leveling-strategy.average-leveling.point2         180.0,0.0 # the second probe point (x,y)
leveling-strategy.average-leveling.point3         270.0,0.0   # the third probe point (x,y)
leveling-strategy.average-leveling.probe_offsets  0,0,9.1       # the probe offsets from nozzle, must be x,y,z, default is no offset
leveling-strategy.average-leveling.tolerance      0.3
leveling-strategy.average-leveling.point_tolerance  0.5
