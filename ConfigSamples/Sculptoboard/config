# Base (fan) auto-on
#-------------------
temperatureswitch.system.enable              true             #
temperatureswitch.system.designator          S                # first character of the temperature control designator to use as the temperature sensor to monitor
temperatureswitch.system.switch              system             # select which switch to use, matches the name of the defined switch
temperatureswitch.system.threshold_temp      40.0             # temperature to turn on (if rising) or off the switch
temperatureswitch.system.heatup_poll         15               # poll heatup at 15 sec intervals
temperatureswitch.system.cooldown_poll       60               # poll cooldown at 60 sec intervals

# Endstops
#---------
endstops_enable                              true             # the endstop module is enabled by default and can be disabled here
alpha_min_endstop                            nc               # add a ! to invert if endstop is NO connected to ground
alpha_max_endstop                            nc               # NOTE set to nc if this is not installed
alpha_homing_direction                       home_to_min      # or set to home_to_max and set alpha_max
alpha_min                                    0                # this gets loaded after homing when home_to_min is set
alpha_max                                    10000              # this gets loaded after homing when home_to_max is set
beta_min_endstop                             nc               #
beta_max_endstop                             3.25!             #
beta_homing_direction                        home_to_max      #
beta_min                                     0                #
beta_max                                     200              #
gamma_min_endstop                            nc             #
#gamma_min_endstop                            3.26             #
gamma_max_endstop                            nc               #
gamma_homing_direction                       home_to_min      #
gamma_min                                    0                #
gamma_max                                    200              #

alpha_fast_homing_rate_mm_s                  25               # feedrates in mm/second
beta_fast_homing_rate_mm_s                   25               # "
gamma_fast_homing_rate_mm_s                  3                # "
alpha_slow_homing_rate_mm_s                  25               # "
beta_slow_homing_rate_mm_s                   10               # "
gamma_slow_homing_rate_mm_s                  2                # "

